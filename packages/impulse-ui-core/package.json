{
  "name": "@impulse-ui/core",
  "version": "2.11.2",
  "license": "MIT",
  "author": "Alexander Polyanov",
  "main": "dist/index.js",
  "module": "dist/core.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint ./src",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "jsxSingleQuote": true,
    "bracketSameLine": false,
    "endOfLine": "auto",
    "bracketSpacing": true
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/core.cjs.production.min.js",
      "limit": "1000 KB"
    },
    {
      "path": "dist/core.esm.js",
      "limit": "1000 KB"
    }
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@floating-ui/react-dom": "^2.0.8",
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-regular-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-table": "^8.15.3",
    "@tanstack/react-virtual": "^3.2.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "styled-components": "6.1.8",
    "stylis": "^4.3.1"
  },
  "devDependencies": {
    "@impulse-ui/types": "^2.11.0"
  },
  "gitHead": "82ece656f5271be949d5e3eff27201cc83ef6555"
}
