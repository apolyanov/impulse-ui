{
  "name": "impulse-ui",
  "version": "1.0.0",
  "license": "MIT",
"type": "module",

  "author": "Alexander Polyanov",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "npm@10.8.1",
  "scripts": {
    "build": "turbo build",
    "watch": "turbo run start",
    "build-storybook": "storybook build",
    "lint": "turbo run lint",
    "storybook": "storybook dev -p 6006",
    "test": "turbo run test",
    "release": "npm run build && npx changeset publish",
    "create-package": "bun scripts/src/createPackage.ts",
    "clean-packages": "bun scripts/src/cleanInstall.ts && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@faker-js/faker": "^9.0.3",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "bun": "^1.1.29",
    "husky": "^9.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.7.0",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@floating-ui/react-dom": "2.1.2",
    "@fontsource/montserrat": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "6.6.0",
    "@fortawesome/free-brands-svg-icons": "6.6.0",
    "@fortawesome/free-regular-svg-icons": "6.6.0",
    "@fortawesome/free-solid-svg-icons": "6.6.0",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@shopify/react-web-worker": "5.1.6",
    "@tanstack/react-table": "8.20.5",
    "@tanstack/react-virtual": "3.10.8",
    "jsqr-es6": "1.4.0-1",
    "lodash-es": "4.17.21",
    "styled-components": "6.1.13",
    "stylis": "4.3.4",
    "tesseract.js": "5.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.22.5"
  }
}
